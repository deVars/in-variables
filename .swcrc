{
  "jsc": {
    "target": "es2020",
    "baseUrl": "./src",
    "loose": false,
    "paths": {},
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
    },
    "transform": {
      "optimizer": {
        "simplify": false,
        "globals": {
          "vars": {
            "window.env": "\"dev\"",
          }
        }
      },
    }
  },
  "module": {
    "type": "es6",
  },
}